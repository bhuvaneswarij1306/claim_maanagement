        <messages/>
        <name>Validation for bill number</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   var reg = /^[0-9]\d{10}/;
   if (!reg.test(newValue)) {
        g_form.setValue('bill_no', '');
        g_form.showErrorBox('bill_no', 'bill number should be numbers');
    }
}

